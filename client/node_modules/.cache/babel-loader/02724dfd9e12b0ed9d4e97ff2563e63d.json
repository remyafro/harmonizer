A2BA,mBAAA,CAAA,KAAA,CAAA,KAAA,eAAA,CA3BA;;AAAA;AA6BA,qBAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA;AA7BA;AAAA;;AAAA;AAAA;AAAA;AAiCA,gBAAA,OAAA,CAAA,GAAA;;AAjCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAqCA,IAAA,aArCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsCA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,KAAA;AAtCA;AAAA,uBAuCA,mBAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,MAAA,CAvCA;;AAAA;AAuCA,gBAAA,CAvCA,GAuCA,KAAA,WAvCA,kBAuCA,IAvCA;;AA8CA,qBAAA,GAAA,IAAA,CAAA,EAAA;AACA,kBAAA,KADA,GACA,CAAA,CAAA,GAAA,CADA;AAEA,kBAAA,YAAA,GAAA,KAAA,CAAA,YAAA;AACA,kBAAA,YAAA,GAAA,KAAA,CAAA,YAAA;AACA,kBAAA,WAAA,GAAA,KAAA,CAAA,WAAA;AACA,kBAAA,YAAA,GAAA,KAAA,CAAA,YAAA;AACA,kBAAA,WAAA,GAAA,KAAA,CAAA,WAAA;AACA;;AAEA,qBAAA,eAAA,GAAA,WAAA;AACA,qBAAA,eAAA,GAAA,QAAA,CAAA,YAAA,CAAA,GAAA,QAAA,CAAA,YAAA,CAAA,GAAA,QAAA,CAAA,WAAA,CAAA,GAAA,QAAA,CAAA,YAAA,CAAA;;AAxDA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA0DA,IAAA,YA1DA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBA2DA,iBAAA,CAAA,OAAA,CAAA,KAAA,UAAA,CAAA,MAAA,CA3DA;;AAAA;AA2DA,gBAAA,CA3DA,GA2DA,KAAA,eA3DA,kBA2DA,IA3DA;;AAgEA,qBAAA,GAAA,IAAA,CAAA,EAAA;AACA,kBAAA,KADA,GACA,CAAA,CAAA,GAAA,CADA;AAEA,kBAAA,CAAA,GAAA,KAAA,CAAA,gBAAA;AACA,kBAAA,CAAA,GAAA,KAAA,CAAA,cAAA;AACA,kBAAA,CAAA,GAAA,KAAA,CAAA,QAAA;AACA;;AACA,qBAAA,6BAAA,GAAA,CAAA,GAAA,CAAA;AACA,qBAAA,uBAAA,GAAA,CAAA,GAAA,CAAA;;AACA,oBAAA,CAAA,GAAA,EAAA,EAAA;AACA,uBAAA,2BAAA,GAAA,KAAA,CAAA,CAAA,GAAA,EAAA,KAAA,IAAA,CAAA,CAAA;AACA,uBAAA,2BAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,2BAAA,CAAA;AACA,iBAHA,MAGA;AACA,uBAAA,2BAAA,GAAA,EAAA;AACA;;AA7EA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,GA7CA;AAgIA,EAAA,QAAA,EAAA;AACA,IAAA,iBAAA,EAAA,6BAAA;AACA,UAAA,SAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,cAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,oBAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,QAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,cAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,YAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,kBAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,cAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,oBAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,UAAA,CAAA,GAAA,QAAA,CAAA,KAAA,UAAA,CAAA,gBAAA,CAAA;;AAEA,UAAA,KAAA,CAAA,SAAA,CAAA,EAAA;AACA,eAAA,CAAA;AACA;;AAEA,aAAA,SAAA;AACA;AATA,GAhIA;AA2IA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA;AA3IA,CAAA","sourcesContent":["<template>\n    <v-app>\n        <v-container\n                class=\"fill-height align-start\"\n        >\n            <v-flex xs6 class=\"mr-4\">\n                <v-alert\n                        v-if=\"error\"\n                        dismissible\n                        type=\"error\">{{ error }}</v-alert>\n                <Panel title=\"Assign Workload\">\n                    <v-card>\n                        <v-card-text>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Assign Load ID\"\n                                    name=\"assignLoadID\"\n                                    type=\"text\"\n                                    required\n                                    :rules=\"[required]\"\n                                    v-model=\"assignLoad.assignLoadID\"\n                            >\n                            </v-text-field>\n                            <v-select\n                                    color=\"dark\"\n                                    label=\"Select Staff\"\n                                    single-line\n                                    :items=\"staffOptions\"\n                                    item-text=\"userName\"\n                                    item-value=\"userID\"\n                                    v-model=\"assignLoad.userID\"\n                                    @change=\"onChangeStaff\"\n\n                            >\n                            </v-select>\n                            <v-select\n                                    color=\"dark\"\n                                    label=\"Select Unit\"\n                                    single-line\n                                    :items=\"unitOptions\"\n                                    item-text=\"unitName\"\n                                    item-value=\"unitID\"\n                                    v-model=\"assignLoad.unitID\"\n                                    @change=\"onChangeUnit\"\n\n                            >\n                            </v-select>\n\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Assignment Hour Assigned to Staff\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.assignmentHour\"\n\n                            >\n\n                            </v-text-field>\n\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Assignment Hour Assigned to Casual\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.assignmentCasualHour\"\n\n                            >\n\n                            </v-text-field>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Exam Hour Assigned to Staff\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.examHour\"\n\n                            >\n\n                            </v-text-field>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Exam Hour Assigned to Casual\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.examCasualHour\"\n\n                            >\n\n                            </v-text-field>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Tutorial Hour Assigned to Staff\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.tutorialHour\"\n\n                            >\n\n                            </v-text-field>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Tutorial Hour Assigned to Casual\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.tutorialCasualHour\"\n\n                            >\n\n                            </v-text-field>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Supp. Assessment Hour Assigned to Staff\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.supAssHour\"\n\n                            >\n\n                            </v-text-field>\n                            <v-text-field\n                                    color=\"dark\"\n                                    label=\"Supp. Assessment Hour Assigned to Casual\"\n                                    required\n                                    :rules=\"[required]\"\n                                    type=\"text\"\n                                    v-model=\"assignLoad.supAssCasualHour\"\n\n                            >\n\n                            </v-text-field>\n                        </v-card-text>\n                        <v-card-actions>\n                            <v-spacer></v-spacer>\n                            <v-btn dark color=\"dark\" @click=\"save\">Save</v-btn>\n                            <v-btn dark color=\"red\" @click=\"deleteAssignLoad\">Delete</v-btn>\n\n                        </v-card-actions>\n                    </v-card>\n                </Panel>\n            </v-flex>\n            <v-flex xs5>\n                <Panel title=\"Current User Selected\"  class=\"mb-5\" v-if=\"currentUser\">\n                    <v-card>\n                        <v-card-text class=\"headline text-left\"\n                                     v-for=\"user in currentUser\" :key=\"user.userID\"\n                        >\n                            <p>Name: {{ user.userName }}</p>\n                            <p>Teaching Hours: {{ user.teachingHour }}</p>\n                            <p>Research Hours: {{ user.researchHour }} </p>\n                            <p>Service Hours: {{ user.serviceHour }} </p>\n                            <p>Misc Hours: {{ user.anythingHour }}</p>\n                            <p>Total: {{ parseInt(user.teachingHour) + parseInt(user.researchHour) + parseInt(user.serviceHour) + parseInt(user.anythingHour) }}</p>\n\n\n                        </v-card-text>\n                    </v-card>\n                </Panel>\n\n                <Panel title=\"Base Hours For Unit\" class=\"mb-5\" v-if=\"currentUnitAssignmentHourLeft\">\n                    <v-card>\n                        <v-card-text class=\"headline text-left\">\n                            <p>Assignment Hours : {{ currentUnitAssignmentHourLeft }} </p>\n                            <p>Exam Hours : {{ currentUnitExamHourLeft }}</p>\n                            <p>Tutorial Hours: {{ currentUnitTutorialHourLeft }}</p>\n                        </v-card-text>\n\n\n                    </v-card>\n                </Panel>\n\n                <Panel title=\"Projected total Hours for this unit\">\n                    <v-card>\n                        <v-card-text class=\"headline\">\n                            <p>Total: {{ currUnitTotalLoad }}</p>\n                        </v-card-text>\n\n\n                    </v-card>\n                </Panel>\n\n\n            </v-flex>\n        </v-container>\n    </v-app>\n</template>\n\n<script>\n    import Panel from '@/components/Panel'\n    import UnitService from \"@/services/UnitService\";\n    import AssignLoadService from \"@/services/AssignLoadService\";\n    import UserWorkLoadService from \"../../services/UserWorkLoadService\";\n    export default {\n        data() {\n            return{\n                assignLoad: {\n                    assignLoadID : null,\n                    assignmentHour: null,\n                    examHour: null,\n                    tutorialHour: null,\n                    supAssHour: null,\n                    assignmentCasualHour: null,\n                    examCasualHour: null,\n                    tutorialCasualHour: null,\n                    supAssCasualHour: null,\n                    userID: null,\n                    unitID: null,\n\n                },\n                required: (value) => !!value || 'Required.',\n                error: null,\n                staff: null,\n                staffOptions : [],\n                unit: null,\n                unitOptions: [],\n                currentUser : null,\n                currentUserLoad: null,\n                currentUnitLoad : null,\n                currentUserType: null,\n\n                currentUnitTutorialHourLeft: null,\n                currentUnitAssignmentHourLeft: null,\n                currentUnitExamHourLeft: null,\n                newUserWorkLoad: null\n            }\n\n        },\n        async mounted(){\n            const assLoadID = this.$store.state.route.params.assignloadid\n            this.assignLoad = (await AssignLoadService.show(assLoadID)).data\n            const currDisc = this.$store.state.discipline;\n            this.unit = (await AssignLoadService.getUnitFromDiscipline(currDisc)).data\n            this.unitOptions = this.unit;\n            this.staff = (await AssignLoadService.getUserFromDiscipline(currDisc)).data\n            this.staffOptions = this.staff;\n            this.currentUnitLoad = (await UnitService.show(this.assignLoad.unitID)).data\n        },\n        methods: {\n            async save() {\n                this.error = null;\n                const areAllFieldsFilledIn = Object\n                    .keys(this.assignLoad)\n                    .every(key => !!this.assignLoad[key])\n                if (!areAllFieldsFilledIn) {\n                    this.error = 'Please fill in all the fields.'\n                    return\n                }\n\n                this.newUserWorkLoad = this.currentUserLoad + parseInt(this.assignLoad.assignmentHour) + parseInt(this.assignLoad.examHour) + parseInt(this.assignLoad.tutorialHour) + parseInt(this.assignLoad.supAssHour)\n                if( this.currentUserType == 'staff-FT' ){\n                    if ( this.newUserWorkLoad > 1380 ){\n                        this.error = 'User has too much teaching workload! Please select another user or assign to casual!'\n                        return\n                    }\n                }\n                if( this.currentUserType == 'staff-PT' ){\n                    if ( this.newUserWorkLoad > 1104 ){\n                        this.error = 'User has too much teaching workload! Please select another user or assign to casual!'\n                        return\n                    }\n                }\n\n                try{\n                    await AssignLoadService.post(this.assignLoad)\n                    await UserWorkLoadService.teach(this.newUserWorkLoad)\n\n                    this.$router.push({\n                        name: 'workload'\n                    })\n                }catch (err) {\n                    console.log(err)\n                }\n\n            },\n            async onChangeStaff() {\n                console.log(this.staff)\n                const b = this.currentUser = (await UserWorkLoadService.show(this.assignLoad.userID)).data\n                var teachingHour\n                var researchHour\n                var serviceHour\n                var anythingHour\n                var accounttype\n\n                for(const key in b){\n                    var value = b[key]\n                    teachingHour = value.teachingHour\n                    researchHour = value.researchHour\n                    serviceHour = value.serviceHour\n                    anythingHour = value.anythingHour\n                    accounttype = value.accountType\n                }\n\n                this.currentUserType = accounttype;\n                this.currentUserLoad = parseInt(researchHour) + parseInt(teachingHour) + parseInt(serviceHour) + parseInt(anythingHour)\n            },\n            async onChangeUnit() {\n                const c = this.currentUnitLoad = (await AssignLoadService.getUnit(this.assignLoad.unitID)).data\n                var z;\n                var x;\n                var y;\n\n                for (const key in c) {\n                    var value = c[key]\n                    z = value.unitTotalStudent;\n                    x = value.assignmentRate;\n                    y = value.examRate;\n                }\n                this.currentUnitAssignmentHourLeft = z * x;\n                this.currentUnitExamHourLeft = z * y;\n                if( z > 50 ){\n                    this.currentUnitTutorialHourLeft = 60 + ( ( z - 50 ) * (1/6) )\n                    this.currentUnitTutorialHourLeft = Math.round(this.currentUnitTutorialHourLeft)\n                }else{\n                    this.currentUnitTutorialHourLeft = 60;\n                }\n\n\n\n            }\n        },\n        computed: {\n            currUnitTotalLoad: function() {\n                const unittotal = parseInt(this.assignLoad.assignmentHour) + parseInt(this.assignLoad.assignmentCasualHour) + parseInt(this.assignLoad.examHour) + parseInt(this.assignLoad.examCasualHour) + parseInt(this.assignLoad.tutorialHour) + parseInt(this.assignLoad.tutorialCasualHour) + parseInt(this.assignLoad.assignmentHour) + parseInt(this.assignLoad.assignmentCasualHour) + parseInt(this.assignLoad.supAssHour) + parseInt(this.assignLoad.supAssCasualHour)\n\n                if(isNaN(unittotal)){\n                    return 0;\n                }\n\n                return unittotal\n            }\n        },\n        components: {\n            Panel\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components/workload"}]}